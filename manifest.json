{
  "manifest_version": 2,
  "name": "bookmarkists",
  "version": "1.0",
  "description": "bookmark manager with gists.github.com",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "default_title",
    "default_popup": "popup/popup.html"
  }
}
